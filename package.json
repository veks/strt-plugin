{
  "name": "strt-plugin",
  "version": "1.0.0",
  "private": true,
  "description": "Плагин для темы Старт.",
  "author": "Veks <oleg@isvek.ru>",
  "license": "GPL-3.0-or-later",
  "type": "module",
  "scripts": {
    "settings-js-compile": "npm-run-all --sequential settings-js settings-js-minify",
    "settings-js": "rollup --config src/build/rollup.settings.js --bundleConfigAsCjs",
    "settings-js-minify": "terser --compress passes=2 --source-map --output assets/js/settings.min.js assets/js/settings.js",
    "settings-js-watch": "nodemon --watch ./src/js/settings --ext js --exec \"npm run settings-js-compile\"",
    "blocks-data-store-js-compile": "npm-run-all --sequential blocks-data-store-js blocks-data-store-js-minify",
    "blocks-data-store-js": "rollup --config src/build/rollup.blocks-data-store.js --bundleConfigAsCjs",
    "blocks-data-store-js-minify": "terser --compress passes=2 --source-map --output assets/js/blocks-data.min.js assets/js/blocks-data.js",
    "blocks-data-store-js-watch": "nodemon --watch ./src/js/data --ext js --exec \"npm run blocks-data-store-js-compile\"",
    "schema-parser-js-compile": "npm-run-all --sequential schema-parser-js schema-parser-js-minify",
    "schema-parser-js": "rollup --config src/build/rollup.schema-parser.js --bundleConfigAsCjs",
    "schema-parser-js-minify": "terser --compress passes=2 --source-map --output assets/js/schema-parser.min.js assets/js/schema-parser.js",
    "admin:admin-settings-css-compile": "npm-run-all --sequential admin:admin-settings-css admin:admin-settings-css-minify",
    "admin:admin-settings-css": "sass --style expanded --source-map --embed-sources src/scss/admin/settings.scss:assets/css/admin-settings.css",
    "admin:admin-settings-css-minify": "sass --style expanded --source-map --embed-sources assets/css/admin-settings.css:assets/css/admin-settings.min.css",
    "admin:admin-settings-css-watch": "nodemon --watch ./src/scss/admin/ --ext scss --exec \"npm run admin:admin-settings-css-compile\"",
    "admin:admin-settings-js-compile": "npm-run-all --sequential admin:admin-settings-js admin:admin-settings-js-minify",
    "admin:admin-settings-js": "rollup --config src/build/rollup.admin.settings.js --bundleConfigAsCjs",
    "admin:admin-settings-js-minify": "terser --compress passes=2 --source-map --output assets/js/admin-settings.min.js assets/js/admin-settings.js",
    "admin:admin-settings-js-watch": "nodemon --watch ./src/js/admin-settings --ext js --exec \"npm run admin:admin-settings-js-compile\"",
    "wp-block-build": "wp-scripts build --config src/build/webpack.blocks.config.js --webpack-copy-php --webpack-src-dir=src/js/blocks --output-path=assets/blocks",
    "wp-block-watch": "wp-scripts start --config src/build/webpack.blocks.config.js --webpack-copy-php --webpack-src-dir=src/js/blocks --output-path=assets/blocks",
    "npm-check-updates": "npx npm-check-updates",
    "ncu-updates": "ncu -u",
    "npm-install": "npm install",
    "npm-install-legacy-peer-deps": "npm install --legacy-peer-deps",
    "npm-cache-clean": "npm cache clean --force",
    "help": "rollup --help"
  },
  "devDependencies": {
    "@babel/cli": "^7.28.0",
    "@babel/core": "^7.28.0",
    "@babel/plugin-transform-runtime": "^7.28.0",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.28.0",
    "@r3flector/react-yandex-maps": "^1.2.7",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-replace": "^6.0.2",
    "@woocommerce/dependency-extraction-webpack-plugin": "^4.0.0",
    "@wordpress/api-fetch": "^7.28.0",
    "@wordpress/base-styles": "^6.4.0",
    "@wordpress/block-editor": "^15.1.0",
    "@wordpress/blocks": "^15.1.0",
    "@wordpress/components": "^30.1.0",
    "@wordpress/compose": "^7.31.0",
    "@wordpress/core-data": "^7.28.0",
    "@wordpress/data": "^10.28.0",
    "@wordpress/dependency-extraction-webpack-plugin": "^6.28.0",
    "@wordpress/element": "^6.28.0",
    "@wordpress/html-entities": "^4.28.0",
    "@wordpress/i18n": "^6.1.0",
    "@wordpress/is-shallow-equal": "^5.28.0",
    "@wordpress/notices": "^5.28.0",
    "@wordpress/scripts": "^30.21.0",
    "@wordpress/server-side-render": "^6.4.0",
    "@wordpress/url": "^4.28.0",
    "ajv": "^8.17.1",
    "ajv-errors": "^3.0.0",
    "ajv-formats": "^3.0.1",
    "axios": "^1.11.0",
    "bootstrap": "^5.3.8",
    "bootstrap-icons": "^1.13.1",
    "clean-css": "^5.3.3",
    "clsx": "^2.1.1",
    "fast-deep-equal": "^3.1.3",
    "fast-sort": "^3.4.1",
    "formik": "^2.4.6",
    "lodash": "^4.17.21",
    "nodemon": "^3.1.10",
    "npm-check-updates": "^18.0.2",
    "npm-run-all": "^4.1.5",
    "p-queue": "^8.1.0",
    "react-bootstrap": "^2.10.10",
    "react-google-recaptcha-v3": "^1.11.0",
    "react-imask": "^7.6.1",
    "react-sortablejs": "^6.1.4",
    "react-toastify": "^11.0.5",
    "react-transition-group": "^4.4.5",
    "rollup": "^4.46.2",
    "rollup-plugin-polyfill-node": "^0.13.0",
    "sass": "^1.90.0",
    "secure-random-password": "^0.2.3",
    "simplebar-react": "^3.3.2",
    "swiper": "^11.2.10",
    "terser": "^5.43.1",
    "use-debounce": "^10.0.5",
    "uuid": "^11.1.0",
    "yet-another-react-lightbox": "^3.25.0",
    "yup": "^1.7.1",
    "zxcvbn": "^4.4.2"
  },
  "dependencies": {
    "core-js": "^3.45.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "regenerator-runtime": "^0.14.1",
    "sortablejs": "^1.15.6"
  }
}
